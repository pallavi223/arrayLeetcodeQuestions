283. Move Zeroes

Input: nums = [0,1,0,3,12]
Output: [1,3,12,0,0]

class Solution {
public:
    void moveZeroes(vector<int>& nums) {
       int i = 0;
        for(int j=0;j<nums.size();j++){
            //whenever you got non-zero value than swap and increment ith position
            if(nums[j] != 0){
                swap(nums[j],nums[i]);
                i++;
            }
        }
       
    }
};
