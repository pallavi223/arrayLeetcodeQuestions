1920. Build Array from Permutation
Input: nums = [0,2,1,5,3,4]
Output: [0,1,2,4,5,3]


class Solution {
public:
    vector<int> buildArray(vector<int>& nums) {
        vector<int> ans(nums.size(), 1);
        int i;
        for(i=0;i<nums.size();i++){
            ans[i] = nums[nums[i]];
           
        }
        
       return ans;
    }
};
