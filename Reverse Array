int arr[] = {11,7,3,1,12,9}



****TWO Pointer Approach:

#include <iostream>
#include <algorithm>

using namespace std;

int main()
{
    int arr[] = {11,7,3,1,12,9};
    int n = 6;
    int s = 0;
    int e = n-1;
    while(s<=e){
        swap(arr[s],arr[e]);
        s++;
        e--;
    }
    
   for(int i=0;i<n;i++){
       cout<<arr[i]<<" ";
   }
    
}


